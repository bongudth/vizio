{
  "name": "vizio",
  "displayName": "vizio",
  "description": "The system automatically generates a flowchart to visualize the code block",
  "version": "0.0.7",
  "publisher": "vizio",
  "icon": "logo.png",
  "repository": "https://github.com/bongudth/vizio",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vizio.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "vizio.generateFlowchart",
        "title": "Generate flowchart"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "vizio.generateFlowchart",
          "group": "vizio",
          "when": "editorTextFocus"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.79.0",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "node-fetch": "^2.6.7"
  }
}
